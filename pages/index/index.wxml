<!--index.wxml-->
<view class="main">
	<swiper  class="swiper" indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" circular="true">
        <block wx:for="{{imgUrls}}" wx:key="{{index}}">
            <swiper-item>
                <image src="{{item.url}}" class="slide-image" width="100%"/>
            </swiper-item>
        </block>
    </swiper>
</view>
